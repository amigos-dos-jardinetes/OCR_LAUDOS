                "secretOptions": []
            },
            "systemControls": []
        }
    ],
    "taskRoleArn": "arn:aws:iam::279634266618:role/ecsTaskExecutionRole",
    "executionRoleArn": "arn:aws:iam::279634266618:role/ecsTaskExecutionRole",
    "networkMode": "bridge",
    "volumes": [],
    "requiresCompatibilities": [
        "EC2"
